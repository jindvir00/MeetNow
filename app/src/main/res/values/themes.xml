    <style name="Theme.MeetNow.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MeetNow.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="Theme.MeetNow.NoActionBar">
        <item name="cornerSize">50%</item>
    </style>
</resources>